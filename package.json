  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/nuxt-shadcn-starter-template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/nuxt-shadcn-starter-template.git"
  },
  "keywords": [
    "nuxt3",
    "vue3",
    "starter-template",
    "dashboard",
    "admin-dashboard",
    "shadcn-vue",
    "shadcn-nuxt",
    "unocss",
    "typescript",
    "tailwind",
    "modern-ui",
    "responsive"
  ],
  "main": "./nuxt.config.ts",
  "files": [
    "app.vue",
    "components",
    "components.json",
    "composables",
    "constants",
    "layouts",
    "lib",
    "nuxt.config.ts",
    "pages",
    "types"
  ],
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "eslint --fix .",
    "setup": "chmod +x setup.sh && ./setup.sh",
    "setup:win": "setup.bat",
    "clean": "chmod +x scripts/clean.sh && ./scripts/clean.sh",
    "clean:win": "scripts\\clean.bat",
    "clean:all": "chmod +x scripts/clean.sh && ./scripts/clean.sh --all",
    "update-deps": "chmod +x scripts/update-deps.sh && ./scripts/update-deps.sh",
    "update-deps:check": "chmod +x scripts/update-deps.sh && ./scripts/update-deps.sh --check",
    "docker:build": "docker build -t nuxt-shadcn-app .",
    "docker:run": "docker run -p 3000:3000 nuxt-shadcn-app",
    "docker:dev": "docker-compose up app-dev",
    "docker:prod": "docker-compose up app"
  },
